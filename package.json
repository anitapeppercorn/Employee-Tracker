  "bugs": {
    "url": "https://github.com/anitapeppercorn/Employee-Tracker/issues"
  },
  "homepage": "https://github.com/anitapeppercorn/Employee-Tracker#readme",
  "dependencies": {
    "asciiart-logo": "^0.2.6",
    "chalk": "^4.0.0",
    "console-clear": "^1.1.1",
    "console.table": "^0.10.0",
    "figlet": "^1.4.0",
    "inquirer": "^7.1.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "jest": "^26.0.1"
  },
  "keywords": [
    "inquirer",
    "node",
    "jest",
    "mySQL"
