{
  "name": "employee-tracker",
  "version": "1.0.0",
  "description": "command-line application to manage a company's employee database using node, inquirer, and MySQL.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "createSchema": "mysql -u root < ./db/schema.sql",
    "seeds": "mysql -u root < ./db/seeds.sql",
    "test": "jest --verbose test/*",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anitapeppercorn/Employee-Tracker.git"
  },
